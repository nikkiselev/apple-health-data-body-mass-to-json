{
  "name": "apple-health-export-bodyweight-to-json",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:cjs": "ncc build src/cli.ts -o dist -m",
    "build:esm": "tsc --target ESNext --module ES6 --outDir esm",
    "watch": "tsc --watch",
    "types:check": "tsc --noEmit",
    "format": "prettier --write \"{src,test,examples}/**/*.{ts,tsx}\"",
    "lint": "eslint \"{src,test,examples}/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"{src,test,examples}/**/*.{ts,tsx}\" --fix",
    "test": "node --max-old-space-size=8192 ./test/index.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run types:check && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint:fix",
      "npm run format",
      "git add"
    ]
  },
  "bin": {
    "apple-health-data-body-mass-to-json": "./dist/cli.js"
  },
  "author": "Nikolai Kiselev",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/minimist": "^1.2.0",
    "@types/node": "^13.9.5",
    "@types/xml2js": "^0.4.5",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "@zeit/ncc": "^0.22.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "lint-staged": "^10.1.3",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "minimist": "^1.2.5",
    "xml2js": "^0.4.23"
  }
}
